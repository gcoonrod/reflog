{
  "name": "reflog-sync-api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:migrate:local": "wrangler d1 execute reflog-sync --local --file=src/db/schema.sql",
    "db:migrate:remote": "wrangler d1 execute reflog-sync --remote --file=src/db/schema.sql"
  },
  "dependencies": {
    "hono": "^4.7.0",
    "jose": "^6.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250214.0",
    "typescript": "^5.7.0",
    "vitest": "~3.2.0",
    "@cloudflare/vitest-pool-workers": "^0.12.0",
    "wrangler": "^4.0.0"
  }
}
